<%- model_class = Establecimiento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:codigo_jurisdiccional) %></th>
      <th><%= model_class.human_attribute_name(:cue) %></th>
      <th><%= model_class.human_attribute_name(:anexo) %></th>
      <th><%= model_class.human_attribute_name(:cue_anexo) %></th>
      <th><%= model_class.human_attribute_name(:sector) %></th>
      <th><%= model_class.human_attribute_name(:ambito) %></th>
      <th><%= model_class.human_attribute_name(:nombre) %></th>
      <th><%= model_class.human_attribute_name(:localidad_id) %></th>
      <th><%= model_class.human_attribute_name(:domicilio) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @establecimientos.each do |establecimiento| %>
      <tr>
        <td><%= link_to establecimiento.id, establecimiento_path(establecimiento) %></td>
        <td><%= establecimiento.codigo_jurisdiccional %></td>
        <td><%= establecimiento.cue %></td>
        <td><%= establecimiento.anexo %></td>
        <td><%= establecimiento.cue_anexo %></td>
        <td><%= establecimiento.sector %></td>
        <td><%= establecimiento.ambito %></td>
        <td><%= establecimiento.nombre %></td>
        <td><%= establecimiento.localidad_id %></td>
        <td><%= establecimiento.domicilio %></td>
        <td><%=l establecimiento.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_establecimiento_path(establecimiento), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      establecimiento_path(establecimiento),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_establecimiento_path,
            :class => 'btn btn-primary' %>
